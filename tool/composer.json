{
    "name": "marellocommerce/marello-dev-tool",
    "description": "Marello dev tools for mono repo",
    "homepage": "https://github.com/marellocommerce/marello-dev-tool.git",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Madia B.V.",
            "homepage": "https://www.marello.com"
        }
    ],
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.org"
        }
    },
    "require": {
        "php": ">=7.1",
        "symfony/console": "~4.2.8",
        "symfony/yaml": "~4.2.8",
        "symfony/process": "~4.2.8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {"": "src/"},
        "exclude-from-classmap": ["/Tests/"]
    }
}
