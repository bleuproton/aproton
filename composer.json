{
    "name": "marellocommerce/marello-subscriptions",
    "description": "Marello Subscriptions extension",
    "homepage": "https://github.com/marellocommerce/marello-subscriptions.git",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Madia B.V.",
            "homepage": "https://www.marello.com"
        }
    ],
    "autoload": {
        "psr-4": {"": "src/"},
        "exclude-from-classmap": ["/Tests/"]
    }
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "marellocommerce/marello": "2.0.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
