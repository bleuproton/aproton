{
    "name": "marellocommerce/marello-orocommerce-bridge",
    "description": "Marello package, integrates Products, Inventory and Orders with OroCommerce",
    "homepage": "https://github.com/marellocommerce/marello-orocommerce-bridge.git",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "Madia B.V.",
            "homepage": "https://www.marello.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "marellocommerce/marello": "self.version"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "2.1-dev"
        }
    },
    "autoload": {
        "psr-4": {"": "src/"},
        "exclude-from-classmap": ["/Tests/"]
    }
}
